@import '../css/_css/bootstrap.min.css'
@import '../css/_css/jquery.fancybox.min.css'

*
	margin 0
	padding 0
	outline none
body
	background-color #fff
	font-family: 'Ubuntu',sans-serif
	font-size: 18px
a
	color: #fefefe
	&:focus
		outline: 0
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
.video_wrapper
	margin 0 auto
	position relative
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #fff
		border-radius 3px
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
.button
	display block
	text-align: center
	border: 2px solid rgb(255, 255, 255)
	border-radius: 5px
	background-color: rgb(254, 153, 0);
	box-shadow: 0px 4px 14.72px 1.28px rgba(0, 0, 0, 0.18);
	width 100%
	max-width: 170px;
	height: 50px;
	line-height: 46px;
	font-size: 16px;
	font-family: "UbuntuBold", sans-serif
	color: rgb(255, 255, 255);
	text-transform: uppercase
	transition: all .15 ease-in
	margin 0 auto
	&:focus
		color: rgb(255, 255, 255);
		background-color: rgb(254, 153, 0);
		text-decoration: none
	&:hover
		color: rgb(255, 255, 255);
		background-color: rgba(255,189,89, 1);
		text-decoration: none
