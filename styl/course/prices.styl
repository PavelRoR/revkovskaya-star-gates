#prices
	width 100%
	padding 60px 0
	background-color: #094b87;
	background-image: url("../img/gives_bg.jpg");
	background-repeat: no-repeat;
	background-position: center;
	background-size: cover;
	box-shadow: 0px 3px 26.68px 2.32px rgba(0,66,97,0.3) inset;
.general_title_prices
	border: 2px solid #fff;
	border-radius: 5px;
	background-color: #01476b;
	box-shadow: 0px 12px 15.66px 2.34px rgba(0,0,0,0.35);
	margin: 0 auto 56px;
	width: 100%;
	max-width: 630px;
	text-align: center;
	font-size: 30px;
	color: #ffe92e;
	line-height: 72px;
#prices_container
	width 100%
	max-width 850px
	border-radius: 5px;
	background-color: rgba(255,255,255,0.9);
	padding: 40px 20px;
	margin: 0 auto;
.price
	width: 100%;
	max-width: 320px;
	margin: 0 auto;
	background-color: #01476b;
	box-shadow: 0px 10px 21.84px 2.16px rgba(0,0,0,0.2);
	border-radius: 5px;
	padding 20px
	border: 1px solid #e88c00
.price_head_title
	text-align: center;
	font-size: 24px;
	font-family: "IntroBlack", sans-serif;
	color: #ffe92e;
	line-height: 1.2;
	text-shadow: 0px 6px 8.46px rgba(0,0,0,0.26);
	position: relative;
	padding-bottom: 20px
	small
		display block
		font-family: 'UbuntuLight',sans-serif
		font-size: 70%
.price_body
	border-top: 1px solid #e88c00
	border-bottom: 1px solid #e88c00
	padding 20px 0
	text-align: center
	color #fff
.price_body_title 
	font-family: 'IntroBoldAlt', sans-serif;
	font-size: 20px
	margin-bottom: 5px
.more_button
	display: inline-block
	border-bottom: 1px dashed
	cursor pointer
	font-size: 16px
	color rgba(255,255,255,.8)
.price_footer
	padding-top: 20px
.cost
	font-size: 24px
	font-family: 'UbuntuBold',sans-serif
	// margin-bottom: 10px
	text-align: center
	color #fff
	span
		display block
		margin-bottom: 15px
.cost_full
	span
		position relative
		&:after
			content ''
			display: block
			height 3px
			width 100px
			background-color: #e88c00
			position: absolute
			left 0
			right 0
			margin 0 auto
			top 17px
			transform: rotate(-8deg)
			box-shadow: 0 0 2px rgba(0,0,0,.4)
			opacity .7
.cost_sale
	color red
.cost_today
	color #ffe92e
.button_cost
	margin-top: 40px
.prepayment
	text-align: center
	margin 25px 0 35px
.prepaymant_title
	font-size: 34px
	color red
	font-family: "IntroBlack", sans-serif
	line-height: 1.3
.prepayment_text
	font-size: 22px
	margin 0 0 20px
.prepayment_button
	max-width 220px
.timer_title
	font-size: 30px
	text-align: center
	font-family: "IntroBlack", sans-serif
	line-height: 1.3
.alert-danger
	color: #a94442;
	background-color: #f2dede;
	padding: 10px;
	width 100%
	max-width 360px
	margin 40px auto 0
	border: 1px solid #ebccd1;
	border-radius: 4px;
	font-size: 24px;
	text-align: center
.flip-clock-wrapper
	width: 100%
	max-width 460px
	margin-top: 40px
.flip-clock-divider .flip-clock-label
	color #333
.flip-clock-dot
	background-color: #0281bc
.flip-clock-wrapper ul li a div div.inn
	color: #ffe92e
	background-color: #0281bc
	text-shadow: 2px 2px 2px rgba(#022a37,.6);