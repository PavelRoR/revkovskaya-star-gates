#results
	width 100%
	padding 60px 0
.results_list
	width 100%
	max-width 860px
	margin 0 auto 30px
	li
		list-style: none
		font-family: "UbuntuLight", sans-serif;
		color: #242424;
		line-height: 1.222;
		margin-bottom: 16px;
		letter-spacing: 0.4px;
		font-size: 18px
		position relative
		padding-left: 15px
		&:last-child
			margin-bottom: 0
		&:before
			content ''
			position: absolute
			display block
			left 0
			top 8px
			border-radius: 50%
			width 8px
			height 8px
			background-color: #fe9900
.results_aftertext
	text-align: center;
	width: 100%;
	max-width: 840px;
	margin: 0 auto;
	font-size: 20px;
	line-height: 1.3;
	color: #fe9900
	letter-spacing: 0.4px;
	span
		display block
		font-family: "UbuntuBold", sans-serif;
		margin-top: 10px